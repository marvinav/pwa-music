{
  "presets": ["@babel/preset-env", "@babel/preset-typescript", "@babel/react"],
  "plugins": [
    ["react-hot-loader/babel"],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    ["@babel/proposal-class-properties"],
    ["@babel/proposal-object-rest-spread"]
  ]
}

